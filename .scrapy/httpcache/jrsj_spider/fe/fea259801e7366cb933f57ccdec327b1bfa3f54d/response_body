function doZoom(size){ var oNews=document.getElementById("newsCon"); oNews.style.fontSize=size;}
function getByClass(oParent, sClass){
	var aEle=oParent;var aResult=[];
	for(var i=0;i<aEle.length;i++){if(aEle[i].className==sClass)aResult.push(aEle[i]);}return aResult;}

function createTab(v){
	var oTit=document.getElementById(v.id).getElementsByTagName("h2");
	var oBox=document.getElementById(v.id).getElementsByTagName('div');
	var thisA=getByClass(oBox, v.setCss);
	for(var i=0;i<oTit.length;i++){oTit[i]._index=i;oTit[i].onmousedown=function (){for(i=0;i<oTit.length;i++){oTit[i].className='';thisA[i].style.display='none';}

this.className=v.cName;thisA[this._index].style.display='block';};}}
</script>
